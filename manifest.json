{
  "manifest_version": 2,
  "name": "Dimtime",
  "version": "1.0",
  "author": "Bartosz Wiorek",
  "description": "Dim page for better browsing experience at night time.",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ]
}
